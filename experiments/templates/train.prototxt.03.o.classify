###########################################################################
# o classification
###########################################################################
layer {
  name: "cls_score_o_int"
  type: "InnerProduct"
  bottom: "score_o"
  top: "cls_score_o_int"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: ${NUM_OUTPUT}
    weight_filler {
      type: "gaussian"
      std: 0.0001
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
